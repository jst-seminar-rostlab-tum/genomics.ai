{
  "/file_download/results": {
    "post": {
      "summary": "Download results",
      "tags": ["Project"],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                }
              },
              "required": ["id"],
              "additionalProperties": false
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "OK"
        },
        "400": {
          "description": "Failed. Bad post data."
        }
      }
    }
  }
}
