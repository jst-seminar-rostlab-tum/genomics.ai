{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": "off",
    "no-console": "off",
    "no-alert": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "import/no-unresolved": "off",
    "import/order": "off"
  },
  "ignorePatterns": [
    "src/components/Visualization"
  ]
}